{
  "compilerOptions": {
    "baseUrl": "./",
    "typeRoots": ["./src/types"],
    "paths": {
      "@src/*": ["src/*"],
      "@config/*": ["src/config/*"],
      "@controllers/*": ["src/controllers/*"],
      "@database/*": ["src/database/*"],
      "@middlewares/*": ["src/middlewares/*"],
      "@modelsExtras/*": ["src/modelsExtras/*"],
      "@routes/*": ["src/routes/*"],
      "@services/*": ["src/services/*"]
    }
  },
  "include": ["./**/*"]
}
